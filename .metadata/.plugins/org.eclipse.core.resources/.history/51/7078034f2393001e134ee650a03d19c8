package javaBasic.ch03.sec02;

public class 확인문제 {

	public static void main(String[] args) {
		
		int x = 10;
		int y = 20;
		int z = (++x) + (y--);   //11 - 20 = 31 
		
		System.out.println(z);
		
		
		int score = 85;
		String result = (!(score>90)) ? "가" : "나";   //가 !부정  !!긍정
		System.out.println(result);
		
		
		

	}

}

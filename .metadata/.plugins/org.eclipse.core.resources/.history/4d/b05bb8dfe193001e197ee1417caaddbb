package javaBasic.ch04.sec01;

public class 확인문제4장 {

	public static void main(String[] args) {
		//p138. 문제3
		//1~100cnffur
		int sum3 = 0; //3의 배수의 합을 넣을 변수
		for(int i =1; i<=100; i++) {
			if(i%3 == 0) {
				sum3 = sum3 + i;
			}
			System.out.println("1부터 100까지 3의 배수 합은 : " + sum3 );
		}
		
	}

}

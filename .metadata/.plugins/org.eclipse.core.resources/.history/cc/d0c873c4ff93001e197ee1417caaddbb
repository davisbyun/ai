package javaBasic2.ch05.sec02;

public class 참조타입2 {

	public static void main(String[] args) {
		
		//A 클래스를 힙메모리에 올리기, 참조변수 aAddr에 저장
		
		 A aAddr = new A();  //이름 국어 평균 변수 올라감
		
		// 홍길동 입력
		 
		 aAddr.name = "홍길동";  	//aAddr을 쫓아가서 거기에 홍길동이라는 이름을 넣어라.
		 aAddr.kor = 100; 		//aAddr을 쫓아가서 거기에 100이라는 숫자를 넣어라.
		 aAddr.avg = 3.5;
		 
		 //p.147
		 A aAddr2 = new A(); //10번지라고 가정하면
		 
		 System.out.println(aAddr == aAddr2);
		 
		 A aAddr3 = aAddr2; //10번지를 aAddr3이라는 변수에 넣어라.
		 System.out.println(aAddr3 == aAddr2);
	}

}

package javaBasic.ch03.sec02;

public class Operator01 {

	public static void main(String[] args) {
		int number = 10;
		//number = number + 1; -->number++과 같음
		//number++;
		++number;
		System.out.println(number);
		
		//number = number - 1; -->number--와 같음 
		//number--;
		--number;
		System.out.println(number);		
		
		int number1 = 100;
		number1++;
		System.out.println(number1);		

	}

}

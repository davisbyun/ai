package javaBasic3.ch06.sec10.exam03;

public class ClassName250 {
	
	//상수
	static final int A = 100;
	
	//변수
	int a = 10;
	
	//인스턴트 맴버들
	
	int field;  //---new필요
	void method(){
		a=a+100;
	};
		 
		
	static int field2;
	static void method2() {};
	
	static {
		
	}
	
	static void method3() {
		//field = 10; 오류 --메모리에 올라가는 시점이 다르다. 
		//method(); 오류
		field2 = 20;
		method2();
		
	}
	
	{
	   field = 10;  
		method(); 
		field2 = 20;
		method2();	
		
	}
	
	void method4() {
	
	    field = 10;  
		method(); 
		field2 = 20;
		method2();	

}
}

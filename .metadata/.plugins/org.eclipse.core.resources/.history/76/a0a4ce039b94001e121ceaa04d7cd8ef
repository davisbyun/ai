package javaBasic2.ch05.sec03;

public class 문자열추출 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String title = "자바프로그램"; //문자열 객체
		String title2 = new String("자바프로그래밍");
		
		System.out.println(title2.length());
		System.out.println(title2.charAt(6));   //밍 (문자열의 인덱스는 0부터 시작)
		
		title2 = title2.replace("자바", "java");  //문자열 변경
		System.out.println(title2);
		
		title2.substring(2, 6);
		System.out.println(title2);
		
	}

}
